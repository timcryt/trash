$set a [@int 2]
$set b [@int 2]
$set c [@int 3]

$set d [@a add @c]
$set e [@c sub @d]
$set f [@a mul @b]
$set g [@d div @a]
$set h [@d rem @a]

$set stdout [$stdout @a @b @c @d @e @f @g @h]

$set inc {[@int $1] add 1}

$set (stdout _) [@while ((0) $stdout 0) {[@int $1] lt 10} {
    $set 1 [$1 @2]
    $set n [@inc $2]
    ((@n) $1 @n)
}]

$set stdout [$stdout [@a eq @b] [@a eq @c] [@a lt @b] [@a lt @c]]
